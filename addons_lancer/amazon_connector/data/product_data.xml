<odoo>
	<data noupdate="1">

        <record id="res_customer_guest_customer" model="res.partner">
            <field name="name">Guest Customer</field>
       	</record>        
        
       	<record id="promotion_discount_product" model="product.product">
            <field name="name">Promotion Discount</field>
            <field name="categ_id" ref="product.product_category_all"/>
            <field name="standard_price">1</field>
            <field name="list_price">1</field>
            <field name="type">service</field>
<!--            <field name="uom_id" ref="product.product_uom_hour"/>-->
<!--            <field name="uom_po_id" ref="product.product_uom_hour"/>-->
            <field name="description">Promotion Discount</field>
        </record>
        
        <record id="shipment_discount_product" model="product.product">
            <field name="name">Shipment Discount</field>
            <field name="categ_id" ref="product.product_category_all"/>
            <field name="standard_price">1</field>
            <field name="list_price">1</field>
            <field name="type">service</field>
<!--            <field name="uom_id" ref="product.product_uom_hour"/>-->
<!--            <field name="uom_po_id" ref="product.product_uom_hour"/>-->
            <field name="description">Shipment Discount</field>
        </record>
        
        <record id="shipment_fee_product" model="product.product">
            <field name="name">Shipping Fee</field>
            <field name="categ_id" ref="product.product_category_all"/>
            <field name="standard_price">1</field>
            <field name="list_price">1</field>
            <field name="type">service</field>
<!--            <field name="uom_id" ref="product.product_uom_hour"/>-->
<!--            <field name="uom_po_id" ref="product.product_uom_hour"/>-->
            <field name="description">Shipping Fee</field>
        </record>
        
        <record id="gift_wrapper_fee_product" model="product.product">
            <field name="name">Gift Wrapper Fee</field>
            <field name="categ_id" ref="product.product_category_all"/>
            <field name="standard_price">1</field>
            <field name="list_price">1</field>
            <field name="type">service</field>
<!--            <field name="uom_id" ref="product.product_uom_hour"/>-->
<!--            <field name="uom_po_id" ref="product.product_uom_hour"/>-->
            <field name="description">Gift Wrapper Fee</field>
        </record>
        
        <!--Sale Shop's data file for Guest Customer-->

		

    </data>
</odoo>


